    {
      "name": "Coffee Grenade",
      "description": "Coffee and pomegranate are two super antioxidant natural ingredients, with a high energizing level. This mocktail is recommended for moments of body recovery after a strong workout.",
      "category": "Spirits",
      "ingredients": [
        {
          "name": "Pomegrade seed",
          "amount": "3 Bar spoons"
        },
        {
          "name": "Pineapple juice",
          "amount": "45 ml"
        },
        {
          "name": "Coffee",
          "amount": "30"
        },
        {
          "name": "Lemon kefir",
          "amount": "Top up"
        },
        {
          "name": "Pineapple wedge",
          "amount": "1"
        }
      ],
      "how_to": [
        {
          "Prepare": "Cut the pineapple wedge for garnish and get the pomegranate seeds"
        },
        {
          "Add Ingredients": "Put the pomegranate seeds and the pineapple juice inside the shaker"
        },
        {
          "Muddle": "Gently smash the pome grenade with the muddler"
        },
        {
          "Ice": "Fill your shaker and glass with cubed ice"
        },
        {
          "Add Coffee Brew": "Add your cold brew coffee 1:5"
        },
        {
          "Shake": "Shake hard for 10 sec"
        },
        {
          "Fine Strain": "Using your fine strainer, strain into your glass over the ice."
        },
        {
          "Top with Lemon Kefir": "Top up the glass with lemon kefir"
        },
        {
          "Garnish and serve": "Garnish with the pineapple wedge"
        }
      ],
